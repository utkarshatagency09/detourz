          </div>
        </div>
          
        {#Quote Icon#}
        <div class="panel mz-panel-default">
          <div class="panel-heading">{{ text_quote_icon }}</div>
          <div class="panel-body">
            <div class="input-image-widget">
              <div class="form-group">
                <label class="col-xs-3 control-label" >{{ entry_icon }}</label>
                <div class="col-xs-9">
                  <ul class="nav nav-tabs">
                    <li class="active"><a href="#tab-testimonial-quote-icon-image" data-meta="image" data-toggle="tab">{{ tab_image }}</a></li>
                    <li><a href="#tab-testimonial-quote-icon-svg" data-meta="image" data-toggle="tab">{{ tab_svg }}</a></li>
                    <li><a href="#tab-testimonial-quote-icon-font" data-meta="font" data-toggle="tab">{{ tab_font }}</a></li>
                  </ul>
                  <div class="tab-content">
                    <div class="tab-pane active" id="tab-testimonial-quote-icon-image">
                      <div class="el-group">
                      {% for language in languages %}
                        <span class="el-item-addon"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /></span>
                        <div class="el-item">
                          <a href="" id="thumb-testimonial-quote-icon-image{{ language.language_id }}" data-toggle="image" class="img-thumbnail"><img src="{{ (thumb_testimonial_quote_icon_image[language.language_id] is defined)?thumb_testimonial_quote_icon_image[language.language_id]:placeholder_image }}" alt="" title="" data-placeholder="{{ placeholder_image }}" /></a><input type="hidden" name="testimonial_quote_icon_image[{{ language.language_id }}]" value="{{ (testimonial_quote_icon_image[language.language_id] is defined)?testimonial_quote_icon_image[language.language_id]:'' }}" id="input-testimonial-quote-icon-image{{ language.language_id }}" />
                        </div>
                      {% endfor %}
                      </div>
                    </div>
                    <div class="tab-pane" id="tab-testimonial-quote-icon-svg">
                      <div class="el-group">
                      {% for language in languages %}
                        <span class="el-item-addon"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /></span>
                        <div class="el-item">
                          <a href="" id="thumb-testimonial-quote-icon-svg{{ language.language_id }}" data-toggle="svg" class="img-thumbnail"><img width="100" height="100" src="{{ (thumb_testimonial_quote_icon_svg[language.language_id] is defined)?thumb_testimonial_quote_icon_svg[language.language_id]:placeholder_svg }}" alt="" title="" data-placeholder="{{ placeholder_svg }}" /></a><input type="hidden" name="testimonial_quote_icon_svg[{{ language.language_id }}]" value="{{ (testimonial_quote_icon_svg[language.language_id] is defined)?testimonial_quote_icon_svg[language.language_id]:'' }}" id="input-testimonial-quote-icon-svg{{ language.language_id }}" />
                        </div>
                      {% endfor %}
                      </div>
                    </div>
                    <div class="tab-pane" id="tab-testimonial-quote-icon-font">
                      <div class="el-group">
                      {% for language in languages %}
                        <span class="el-item-addon"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /></span>
                        <div class="el-item">
                          <a href="" id="thumb-testimonial-quote-icon-font{{ language.language_id }}" data-toggle="font" class="img-thumbnail icon-thumb"><i class="{{ (testimonial_quote_icon_font[language.language_id] is not empty)?testimonial_quote_icon_font[language.language_id]:placeholder_font }}" data-placeholder="{{ placeholder_font }}"></i></a><input type="hidden" name="testimonial_quote_icon_font[{{ language.language_id }}]" value="{{ (testimonial_quote_icon_font[language.language_id] is defined)?testimonial_quote_icon_font[language.language_id]:'' }}" id="input-testimonial-quote-icon-font{{ language.language_id }}" />
                        </div>
                      {% endfor %}
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="image-meta-tab-content tab-content">
                <div class="tab-image active tab-pane">
                  {# Icon width#}
                  <div class="form-group">
                    <label class="col-xs-3 control-label" for="input-testimonial-quote-icon-width">{{ entry_width }}</label>
                    <div class="col-xs-9">
                      <input type="number" name="testimonial_quote_icon_width" value="{{ testimonial_quote_icon_width }}" placeholder="{{ entry_width }}" id="input-testimonial-quote-icon-width" class="form-control" />
                    </div>
                  </div>

                  {# Icon height#}
