                    <label class="col-xs-3 control-label" for="input-testimonial-quote-icon-height">{{ entry_height }}</label>
                    <div class="col-xs-9">
                      <input type="number" name="testimonial_quote_icon_height" value="{{ testimonial_quote_icon_height }}" placeholder="{{ entry_height }}" id="input-testimonial-quote-icon-height" class="form-control" />
                    </div>
                  </div>
                </div>
                <div class="tab-font tab-pane">
                  <!--icon size-->
                  <div class="form-group">
                    <label class="col-xs-3 control-label" for="input-testimonial-quote-icon-size">{{ entry_size }}</label>
                    <div class="col-xs-9">
                      <input type="number" name="testimonial_quote_icon_size" value="{{ testimonial_quote_icon_size }}" placeholder="{{ entry_size }}" class="form-control" id="input-testimonial-quote-icon-size" />
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
