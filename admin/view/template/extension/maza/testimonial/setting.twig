                    <label for="input-form-rating-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% else %}
                    <input type="radio" name="testimonial_form_rating" value="{{ field['id'] }}" id="input-form-rating-{{ field['id'] }}">
                    <label for="input-form-rating-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% endif %}
                  {% endfor %}
                </div>
              </div>
            </div>
                
            {# Email #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_email }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% for field in list_field_status %}
                    {% if (testimonial_form_email == field['id']) %} 
                    <input type="radio" name="testimonial_form_email" value="{{ field['id'] }}" id="input-form-email-{{ field['id'] }}" checked>
                    <label for="input-form-email-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% else %}
                    <input type="radio" name="testimonial_form_email" value="{{ field['id'] }}" id="input-form-email-{{ field['id'] }}">
                    <label for="input-form-email-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% endif %}
                  {% endfor %}
                </div>
              </div>
            </div>
                
            {# Image #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_image }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% for field in list_field_status %}
                    {% if (testimonial_form_image == field['id']) %} 
                    <input type="radio" name="testimonial_form_image" value="{{ field['id'] }}" id="input-form-image-{{ field['id'] }}" checked>
                    <label for="input-form-image-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% else %}
                    <input type="radio" name="testimonial_form_image" value="{{ field['id'] }}" id="input-form-image-{{ field['id'] }}">
                    <label for="input-form-image-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% endif %}
                  {% endfor %}
                </div>
              </div>
            </div>
                
            {# Extra #}
            <div class="form-group">
              <label class="col-xs-3 control-label" ><span data-toggle="tooltip" title="{{ help_extra }}">{{ entry_extra }}</span></label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% for field in list_field_status %}
                    {% if (testimonial_form_extra == field['id']) %} 
                    <input type="radio" name="testimonial_form_extra" value="{{ field['id'] }}" id="input-form-extra-{{ field['id'] }}" checked>
                    <label for="input-form-extra-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% else %}
                    <input type="radio" name="testimonial_form_extra" value="{{ field['id'] }}" id="input-form-extra-{{ field['id'] }}">
                    <label for="input-form-extra-{{ field['id'] }}" >{{ field['text'] }}</label>
                    {% endif %}
                  {% endfor %}
                </div>
              </div>
            </div>
                
            {# captcha #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_captcha }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% if (testimonial_form_captcha) %} 
                  <input type="radio" name="testimonial_form_captcha" value="1" id="input-form-captcha-enabled" checked>
                  <label for="input-form-captcha-enabled" >{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_form_captcha" value="0" id="input-form-captcha-disabled">
                  <label for="input-form-captcha-disabled">{{ text_disabled }}</label>
                  {% else %} 
                  <input type="radio" name="testimonial_form_captcha" value="1" id="input-form-captcha-enabled" >
                  <label for="input-form-captcha-enabled">{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_form_captcha" value="0" id="input-form-captcha-disabled" checked>
                  <label for="input-form-captcha-disabled" >{{ text_disabled }}</label>
                  {% endif %} 
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
        
      {# Listing #}
      <div id="tab-mz-listing" class="tab-pane">
        {#General#}
        <div class="panel mz-panel-default">
          <div class="panel-heading">{{ text_general }}</div>
          <div class="panel-body">
            {# Rating #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_rating }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
