                  <input type="radio" name="testimonial_list_rating" value="1" id="input-list-rating-enabled" checked>
                  <label for="input-list-rating-enabled" >{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_list_rating" value="0" id="input-list-rating-disabled">
                  <label for="input-list-rating-disabled">{{ text_disabled }}</label>
                  {% else %} 
                  <input type="radio" name="testimonial_list_rating" value="1" id="input-list-rating-enabled" >
                  <label for="input-list-rating-enabled">{{ text_enabled }}</label>
