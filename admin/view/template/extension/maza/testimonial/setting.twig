                  <label for="input-list-timestamp-disabled" >{{ text_disabled }}</label>
                  {% endif %} 
                </div>
              </div>
            </div>
                
            {# Image #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_image }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% if (testimonial_list_image) %} 
                  <input type="radio" name="testimonial_list_image" value="1" id="input-list-image-enabled" checked>
                  <label for="input-list-image-enabled" >{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_list_image" value="0" id="input-list-image-disabled">
                  <label for="input-list-image-disabled">{{ text_disabled }}</label>
                  {% else %} 
                  <input type="radio" name="testimonial_list_image" value="1" id="input-list-image-enabled" >
                  <label for="input-list-image-enabled">{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_list_image" value="0" id="input-list-image-disabled" checked>
                  <label for="input-list-image-disabled" >{{ text_disabled }}</label>
                  {% endif %} 
                </div>
              </div>
            </div>
                
            {# Extra #}
            <div class="form-group">
              <label class="col-xs-3 control-label" ><span data-toggle="tooltip" title="{{ help_extra }}">{{ entry_extra }}</span></label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% if (testimonial_list_extra) %} 
                  <input type="radio" name="testimonial_list_extra" value="1" id="input-list-extra-enabled" checked>
                  <label for="input-list-extra-enabled" >{{ text_enabled }}</label>
