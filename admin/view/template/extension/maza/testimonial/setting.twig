{{ mz_header }}
<div id="content" class="mz-content">
  
  <!-- Alert message -->
  {% if (success is defined) %} 
    <div class="alert alert-success"><i class="fa fa-thumbs-o-up" aria-hidden="true"></i> {{ success }} <a href="#" class="close" data-dismiss="alert" aria-label="close"><i class="fa fa-times-circle" aria-hidden="true"></i></a></div>
  {% endif %} 
  {% if (warning is defined) %} 
    <div class="alert alert-warning"><i class="fa fa-exclamation-triangle" aria-hidden="true"></i> {{ warning }} <a href="#" class="close" data-dismiss="alert" aria-label="close"><i class="fa fa-times-circle" aria-hidden="true"></i></a></div>
  {% endif %} 
  
  <!-- Form -->
  <form action="{{ action }}" class="form-horizontal" enctype="multipart/form-data" id="form-mz-setting" method="post">
    <div class="tab-content">
      <div id="tab-mz-setting" class="tab-pane active">
        {# Testimonial submit Form #}
        <div class="panel mz-panel-default">
          <div class="panel-heading">{{ text_submit_testimonial }}</div>
          <div class="panel-body">
            {# submit status #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_status }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% if (testimonial_submit_status) %} 
                  <input type="radio" name="testimonial_submit_status" value="1" id="input-submit-status-enabled" checked>
                  <label for="input-submit-status-enabled" >{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_submit_status" value="0" id="input-submit-status-disabled">
                  <label for="input-submit-status-disabled">{{ text_disabled }}</label>
                  {% else %} 
                  <input type="radio" name="testimonial_submit_status" value="1" id="input-submit-status-enabled" >
                  <label for="input-submit-status-enabled">{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_submit_status" value="0" id="input-submit-status-disabled" checked>
                  <label for="input-submit-status-disabled" >{{ text_disabled }}</label>
                  {% endif %} 
                </div>
              </div>
            </div>
                
            {# Require approval of new testimonial #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_require_approval }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% if (testimonial_submit_require_approval) %} 
                  <input type="radio" name="testimonial_submit_require_approval" value="1" id="input-submit-require-approval-enabled" checked>
                  <label for="input-submit-require-approval-enabled" >{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_submit_require_approval" value="0" id="input-submit-require-approval-disabled">
                  <label for="input-submit-require-approval-disabled">{{ text_disabled }}</label>
                  {% else %} 
                  <input type="radio" name="testimonial_submit_require_approval" value="1" id="input-submit-require-approval-enabled" >
                  <label for="input-submit-require-approval-enabled">{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_submit_require_approval" value="0" id="input-submit-require-approval-disabled" checked>
                  <label for="input-submit-require-approval-disabled" >{{ text_disabled }}</label>
                  {% endif %} 
                </div>
              </div>
            </div>
                
            {# thank you mail status #}
            <div class="form-group">
              <label class="col-xs-3 control-label" >{{ entry_thank_you_mail }}</label>
              <div class="col-xs-9">
                <div class="mz-switch-field">
                  {% if (testimonial_mail_status) %} 
                  <input type="radio" name="testimonial_mail_status" value="1" id="input-mail-status-enabled" checked>
                  <label for="input-mail-status-enabled" >{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_mail_status" value="0" id="input-mail-status-disabled">
                  <label for="input-mail-status-disabled">{{ text_disabled }}</label>
                  {% else %} 
                  <input type="radio" name="testimonial_mail_status" value="1" id="input-mail-status-enabled" >
                  <label for="input-mail-status-enabled">{{ text_enabled }}</label>
                  <input type="radio" name="testimonial_mail_status" value="0" id="input-mail-status-disabled" checked>
                  <label for="input-mail-status-disabled" >{{ text_disabled }}</label>
                  {% endif %} 
                </div>
              </div>
            </div>
          </div>
        </div>
                
        {#Testimonial submit mail#}
        <div class="panel mz-panel-default">
          <div class="panel-heading clearfix">
            <h4 class="pull-left panel-title">{{ text_mail_template }}</h4>
            <ul class="nav nav-tabs tab-dynamic pull-right mz-nav-sandwich-tab">
              {% for language in languages %}
              <li><a href="#language-mail-template{{ language.language_id }}" data-toggle="tab"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /> {{ language.name }}</a></li>
              {% endfor %}
            </ul>
          </div>
          <div class="panel-body">
            <div class="tab-content">
              {% for language in languages %}
              <div class="tab-pane" id="language-mail-template{{ language.language_id }}">
                <div class="form-group">
                  <label class="col-xs-2 control-label" for="input-testimonial-mail-template-subject{{ language.language_id }}">{{ entry_subject }}</label>
                  <div class="col-xs-10">
                    <input type="text" name="testimonial_mail_template[{{ language.language_id }}][subject]" value="{{ testimonial_mail_template[language.language_id]['subject'] is defined ? testimonial_mail_template[language.language_id]['subject'] }}"  id="input-testimonial-mail-template-subject{{ language.language_id }}" placeholder="{{ entry_subject }}" class="form-control large" />
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-xs-2 control-label" for="input-testimonial-mail-template-message{{ language.language_id }}">{{ entry_message }}</label>
                  <div class="col-xs-10">
                    <textarea name="testimonial_mail_template[{{ language.language_id }}][message]" placeholder="{{ entry_message }}" id="input-testimonial-mail-template-message{{ language.language_id }}" data-ckeditor="{{ code }}" class="form-control">{{ testimonial_mail_template[language.language_id]['message'] is defined ? testimonial_mail_template[language.language_id]['message'] : '' }}</textarea>
                  </div>
                </div>
              </div>
              {% endfor %}
            </div>
          </div>
        </div>
                
      </div>
            
      {# form #}
      <div id="tab-mz-form" class="tab-pane">
        <div class="panel mz-panel-default">
          <div class="panel-body">
            {# Rating #}
            <div class="form-group">
